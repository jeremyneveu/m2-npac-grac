pages = {044032},
title = {{Time-dependent spherically symmetric covariant Galileons}},
url = {http://link.aps.org/doi/10.1103/PhysRevD.87.044032 http://arxiv.org/abs/1212.1394},
volume = {87},
year = {2013}
}
