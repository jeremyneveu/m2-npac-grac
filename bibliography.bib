issn = {1550-7998},
journal = {Physical Review D},
mendeley-groups = {Th\'{e}orie/Marc papers},
month = nov,
number = {10},
pages = {103506},
title = {{Possible dark matter origin of the gamma ray emission from the Galactic Center observed by HESS}},
url = {http://arxiv.org/abs/1204.0655 http://link.aps.org/doi/10.1103/PhysRevD.86.103506},
volume = {86},
