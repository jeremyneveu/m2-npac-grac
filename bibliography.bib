journal = {The Astrophysical Journal},
mendeley-groups = {Cosmologie/Supernovae},
month = feb,
number = {2},
pages = {661--665},
primaryClass = {astro-ph},
title = {{On the origin of the Type Ia Supernova width-luminosity relation}},
url = {http://arxiv.org/abs/astro-ph/0609540},
volume = {656},
year = {2007}
}
